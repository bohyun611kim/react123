						if(remainQtyPer > payMaxQty) {
							$('#joinQty').val(prcInfo.payCoinMaxQty);
							qty = prcInfo.payCoinMaxQty;
							
							let exchangeQty = ((qty * 1000000000) / (prcInfo.payCoinBasicQty * 1000000000)) * prcInfo.slCoinBasicQty;
							bonusQty = exchangeQty * bonus / 100;
							$('#exchangeQty').val(nu.cm((exchangeQty + bonusQty), decimalPnt));
						} 
					}	
				} else if(uesrRemainQtyPer < exchangeQty){
					
					if(bonus != 0){
						uesrBonusQty = uesrRemainQtyPer * bonus / 100;
						uesrRemainQtyPer = uesrRemainQtyPer + uesrBonusQty;
						
						qty = uesrRemainQtyPer;
						$('#exchangeQty').val(nu.cm(qty, decimalPnt));
						qty = qty - uesrBonusQty;
						let joinQty = (qty*prcInfo.payCoinBasicQty) / prcInfo.slCoinBasicQty;
						$('#joinQty').val(nu.cm(joinQty, bal.pnt));
					} else {		
						qty = uesrRemainQtyPer;
						let joinQty = (qty*prcInfo.payCoinBasicQty) / prcInfo.slCoinBasicQty
							, remaind = ( nu.remainder(joinQty, prcInfo.payCoinUnitQty) );
						
						joinQty = (joinQty - remaind);
						qty =  ((joinQty * 1000000000) / (prcInfo.payCoinBasicQty * 1000000000)) * prcInfo.slCoinBasicQty;	
						
						$('#exchangeQty').val(nu.cm(qty, decimalPnt));
						$('#joinQty').val(nu.cm(joinQty, bal.pnt));	
						
						if(uesrRemainQtyPer > payMaxQty) {
							$('#joinQty').val(prcInfo.payCoinMaxQty);
							qty = prcInfo.payCoinMaxQty;
							
							let exchangeQty = ((qty * 1000000000) / (prcInfo.payCoinBasicQty * 1000000000)) * prcInfo.slCoinBasicQty;
							bonusQty = exchangeQty * bonus / 100;
							$('#exchangeQty').val(nu.cm((exchangeQty + bonusQty), decimalPnt));
						} 
					}
					
				} else if(uesrRemainQtyPer > exchangeQty){
					
					if(bonus != 0){						
						qty = ((bal.qty * 1000000000) - ( nu.remainder(bal.qty, prcInfo.payCoinUnitQty) *1000000000 )) / 1000000000;
						$('#joinQty').val(nu.cm(qty, bal.pnt));
						
						uesrBonusQty = qty * bonus / 100;
						qty = qty + uesrBonusQty;
						let joinQty = ((qty * 1000000000) / (prcInfo.payCoinBasicQty * 1000000000)) * prcInfo.slCoinBasicQty;
						$('#exchangeQty').val(nu.cm(joinQty, decimalPnt));	
					} else {		
					/* 	let coinUnitQty = prcInfo.payCoinUnitQty.toString().split('.')
						, count = coinUnitQty[1].length;
						qty = (bal.qty - ( nu.remainder(bal.qty, prcInfo.payCoinUnitQty) )).toFixed(count); */
						qty = ((bal.qty * 1000000000) - ( nu.remainder(bal.qty, prcInfo.payCoinUnitQty) *1000000000 )) / 1000000000;
						$('#joinQty').val(nu.cm(qty, bal.pnt));
						let joinQty = ((qty * 1000000000) / (prcInfo.payCoinBasicQty * 1000000000)) * prcInfo.slCoinBasicQty;
