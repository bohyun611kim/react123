			, authCode  = ''
			, qty = $('#joinQty').val().replace( /,/gi, '')
			, coinNo = $('#coinType').val()
			, prcInfo = priceInfo[coinNo]
			, authMeansCd = '${info.getAuthMeansCd()}'
			, exchangeQty = $('#exchangeQty').val();
		
