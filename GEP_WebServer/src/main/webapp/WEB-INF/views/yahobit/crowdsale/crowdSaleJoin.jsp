			authCode = sms;
		else
			authCode = otp;
			
		authCode = $.trim(authCode.val());
		
		if(G__prvSubmit == true) return false;
		
		if(authMeansCd == 0) {
			lrt.client('보안 인증 수단을 등록하세요', '알림');
