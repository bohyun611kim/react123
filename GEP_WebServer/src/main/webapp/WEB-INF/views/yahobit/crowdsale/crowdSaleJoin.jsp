			return false;
		} else if(qty < prcInfo.payCoinMinQty) {
			lrt.client('1회 당 최소 참여 수량은 ' + prcInfo.payCoinMinQty + ' 입니다.');
			qty = prcInfo.payCoinMinQty;
			return false;
		} else if(qty > prcInfo.payCoinMaxQty) {
			lrt.client('1회 당 최대 참여 수량은 ' + prcInfo.payCoinMaxQty + ' 입니다.');
			qty = prcInfo.payCoinMaxQty;
			return false;
		} else if(!checkbox.eq(1).is(':checked')) {
			lrt.client('이용약관에 동의해야 합니다', '알림');
			return false;
		} else if(!checkbox.eq(2).is(':checked')) {
			lrt.client('주의사항을 읽고 확인해야 합니다', '알림');
			return false;
		} if(authCode == '' || authCode.length != 6) {
			lrt.client('인증번호 6자리를 입력하세요', '알림');
			return false;
		}
		
		ajaxOption({
			url:'/alibit/crowdSale/joinCrowd',
			data:{
				no:parseFloat('${info.getNo()}'),
				type:$('#coinType').val(),
				slType:'${info.getSlCoinNo()}',
				qty:qty,
				authCode:authCode
			},
			success:function(data) {
				if(data.rtnCd == 0) {
					G__prvSubmit = true;
					lrt.client('신청 되었습니다', '알림', function() {
						location.reload();
					}, null);
				} else {
					if(wordbook[data.rtnCd] != null){
						lrt.client(wordbook[data.rtnCd], '알림');		
					} else {				
						lrt.client(data.rtnMsg, '알림');
					}
				}
			}
		});
	});
})

function getPriceInfo() {
	ajaxOption({
		url:'/alibit/crowdSale/getPriceInfo',
		data:{no:parseFloat('${info.getNo()}')},
		success:function(data) {
			let price = data.price, prcHtml = '';
			
			for(let i=0; i<data.balance.length; i++) {
				let temp =  data.balance[i];
				balance[temp.coinNo] = temp;
			}
			
			for(let i=0; i<price.length; i++) {
				let temp = price[i];
				priceInfo[temp.payCoinNo] = temp;
				prcHtml += '<option value="' + temp.payCoinNo + '">' + temp.symbol + '</option>';
			}
			$('#coinType').html(prcHtml);
			
			let $span = $('#balance').children('span')
				, bal = balance[price[0].payCoinNo];
			
			$span.eq(0).text(nu.cm(bal.qty, bal.pnt));
			$span.eq(1).text(bal.symbol);
		}
	});
}

function isChecked(target) {
	return target.is(':checked') ? 1:0;
}

function checkFileSize(selector) {
	let max = 6*1024*1024;
	if(selector[0].files[0].size > max) {
		return false;
	} else {
		return true;
	}
}

function checkExtension(selector) {
	let fileName = selector[0].files[0].name;
	let index = fileName.lastIndexOf('.');

	if(index != -1) {
		let ext = fileName.substr(index+1).toLocaleLowerCase();
		
		if(ext === 'jpg' || ext === 'png') {
			return true;
		} else {
			return false;
		}
	}
}

//maxlengthCheck
//실수의 정수부분과 소수부분 자릿수 제한
//maxlength: 정수부분 최대 자릿수, 미정의시 14자리
//maxdeclength: 소수부분 최대 자릿수, 미정의시 8자리
function maxlengthcheck(object, maxlength, maxdeclength, floatYN) {
	
	var ml = maxlength == undefined ? 14 : maxlength 
	var mdl = maxdeclength == undefined ? 8 : maxdeclength
	var v = $(object)
	
	if( v.val().split('.')[0].length > ml )
		object.value = v.val().split('.')[0].slice(0, ml) + (v.val().indexOf('.') > -1 ? '.'+v.val().split('.')[1] : '')

	if( v.val().indexOf('.') > -1 )
		if( v.val().split('.')[1].length > mdl )
			object.value = v.val().split('.')[0] +'.'+ v.val().split('.')[1].slice(0, mdl)

}

